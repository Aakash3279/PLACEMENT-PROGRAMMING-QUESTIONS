public class Main {
    public static void main(String[] args) {
        // Positive number
        int number = 60;
        System.out.print("Factors of " + number + " are: ");

        // Loop runs from 1 to 60
        for (int i = 1; i <= number; ++i) {
            // If number is divided by i, i is a factor
            if (number % i == 0) {
                System.out.print(i + " ");
            }
        }
    }
}

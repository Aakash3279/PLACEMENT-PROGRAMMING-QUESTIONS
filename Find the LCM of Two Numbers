import java.util.*;
public class Main {
    public static void main(String[] args) {
        //lcm of two numbers
        Scanner sc = new Scanner(System.in);
        int a = sc.nextInt();
        int b = sc.nextInt();
        int Gcd = 1;
        for(int i = 1;i<=a && i<=b;i++){
            if(a%i == 0 && b%i == 0){
                Gcd = i;
            }
        }
        int lcm = (a * b)/Gcd;
        System.out.println("the Lcm of "+a+" and "+b+" is "+lcm);
    }
}

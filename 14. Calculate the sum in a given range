import java.util.*;
public class Main {
    public static int sum(int s,int e){
        int sum = 0;
        for(int i = s;i<=e;i++){
            sum += i;
        }
        return  sum;
    }
    public static void main(String[] args) {
        Scanner sc = new Scanner(System.in);
        System.out.print("Start : ");
        int s = sc.nextInt();
        System.out.print("End : ");
        int e = sc.nextInt();
        System.out.println(sum(s,e));
    }
}
